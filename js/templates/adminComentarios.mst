<table class="table table-hover">
  <thead>
    <td>#</td>
    <td>Paquete</td>
    <td>Usuario</td>
    <td>Mensaje</td>
    <td></td>
  </thead>
  <tbody>
    {{#comentarios}}
    <tr>
      <td>{{@index}}</td>
      <td>{{paquete}}</td>
      <td>{{nombreUsuario}}</td>
      <td>{{mensaje}}</td>
      <td><button class="btn btn-danger eliminarComentario" data-idcomentario="{{id_comentario}}" type="button">Eliminar</button></td>
    </tr>
    {{/comentarios}}
  </tbody>
</table>
